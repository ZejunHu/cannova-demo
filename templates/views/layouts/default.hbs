						<li><a href="https://www.instagram.com/cannovaimmigration/" target="_blank"><i class="fa fa-instagram"></i></a></li>
						<li><a href="https://www.weibo.com/6377141436" target="_blank"><i class="fa fa-weibo"></i></a></li>
						<li class="footer-wechat">
							<a data-toggle="modal" href="#footer-wechat">
								<i class="fa fa-wechat"></i>
							</a>
						</li>
					</ul>
					<!-- /footer follow -->

					<!-- footer copyright -->
					<div class="footer-copyright">
						<p>Copyright Â© 2018. All Rights Reserved. Designed by <a href="http://savvyrocks.ca/" target="_blank">Savvy Media Inc.</a></p>
					</div>
					<!-- /footer copyright -->

				</div>

			</div>
			<!-- /Row -->

		</div>
